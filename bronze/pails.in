# Background & Color Match

word = 'Bunnybunnybunny'
colors = [Fore.LIGHTRED_EX + Back.RED, Fore.LIGHTYELLOW_EX + Back.YELLOW, Fore.LIGHTGREEN_EX+ Back.GREEN, Fore.LIGHTCYAN_EX + Back.CYAN, Fore.BLUE, Fore.MAGENTA]
